<http://data.collectienederland.nl/resource/document/mauritshuis/253> schema:IsBasedOn <http://data.collectienederland.nl/resource/document/mauritshuis/253> ;
    schema:creator "Jan Brueghel de Oude",
        "Jan Brueghel the Elder",
